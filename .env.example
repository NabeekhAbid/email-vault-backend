# MySQL Database Configuration
MYSQL_HOST=127.0.0.1                         # Host where MySQL is running
MYSQL_PORT=3306                              # MySQL port (default: 3306)
MYSQL_USER=root                              # MySQL username
MYSQL_PASSWORD=your_mysql_password           # MySQL password
MYSQL_DATABASE=vault                         # Name of the database

# RabbitMQ Configuration
RABBITMQ_HOST=127.0.0.1                      # Host where RabbitMQ is running
RABBITMQ_PORT=5672                           # RabbitMQ port (default: 5672)
RABBITMQ_USER=guest                          # RabbitMQ username
RABBITMQ_PASSWORD=your_rabbitmq_password     # RabbitMQ password
RABBITMQ_VIRTUAL_HOST=/                      # RabbitMQ virtual host

# Frontend URL Configuration
FRONT_END_BASE_URL=http://localhost:3004     # Base URL of the frontend for email links

# Mailjet API Configuration
MAILJET_API_KEY=your_mailjet_api_key         # Mailjet API key for sending emails
MAILJET_API_SECRET=your_mailjet_api_secret   # Mailjet API secret for sending emails

# Queue Configuration
EmailServiceProcessor_QUEUE_NAME=email_queue              # Name of the email processing queue
QUEUE_NAME_PREFIX=email_vault_backend_                    # Prefix for queues used by the backend
